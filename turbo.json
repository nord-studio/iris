{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": [
		".env"
	],
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				".next/**",
				"!.next/cache/**",
				"target/**"
			]
		},
		"dev": {
			"persistent": true,
			"cache": false,
			"inputs": [
				".env"
			]
		},
		"drizzle": {
			"persistent": false,
			"cache": false
		}
	}
}